    "[STD_CylConstant_RCP_RI_2] = '" & TagSrc.STD_CylConstant_RCP_RI_2.Value & "',[STD_CylConstant_RCP_RI_3] = '" & TagSrc.STD_CylConstant_RCP_RI_3.Value & "',[STD_CylConstant_RCP_RI_4] = '" & TagSrc.STD_CylConstant_RCP_RI_4.Value & "',[STD_CylConstant_RCP_RI_6] = '" & TagSrc.STD_CylConstant_RCP_RI_6.Value & "',[STD_CylConstant_RCP_RI_7] = '" & TagSrc.STD_CylConstant_RCP_RI_7.Value & "',[STD_CylConstant_RCP_RI_8] = '" & TagSrc.STD_CylConstant_RCP_RI_8.Value & "', [STD_CylConstant_RCP_RI_9] = '" & TagSrc.STD_CylConstant_RCP_RI_9.Value & "',[STD_CylConstant_RCP_RI_10] = '" & TagSrc.STD_CylConstant_RCP_RI_10.Value & "',[indB_STD_SpareInt_RCP_RI_1] = '" & TagSrc.STD_SpareInt_RCP_RI_1.Value & "',[indB_STD_SpareInt_RCP_RI_2] = '" & TagSrc.STD_SpareInt_RCP_RI_2.Value & "' " & _
    "WHERE RcpID = '" & RcpID & "'"
    cmd.ActiveConnection = cnSrc
    rsSrc.Open strSql, cnSrc
   
