    strSql = " INSERT INTO Inducto_A VALUES('"
    strSql = strSql & RcpID & "','" & TagSrc.Q_inda_EXRATE.Value & "','" & TagSrc.RcpGrade.Value & "','" & TagSrc.Q_inda_MINTEMP.Value & "','" & TagSrc.Q_inda_RATE.Value & "','" & TagSrc.Q_inda_RBARS.Value & "','" & TagSrc.Q_inda_SQBARS.Value & "','" & TagSrc.Q_inda_TEMP.Value & "','" & TagSrc.Q_inda_bil_SIZE.Value & "','"
    strSql = strSql & TagSrc.RI_DCH_RcpSET_1.Value & "','" & TagSrc.RI_DCH_RcpSET_2.Value & "','" & TagSrc.RI_DCH_RcpSET_3.Value & "','" & TagSrc.RI_DCH_RcpSET_4.Value & "','" & TagSrc.RI_DCH_RcpSET_5.Value & "','" & TagSrc.RI_DCH_RcpSET_6.Value & "','" & TagSrc.RI_DCH_RcpSET_7.Value & "','" & TagSrc.RI_DCH_RcpSET_8.Value & "','" & TagSrc.RI_DCH_RcpSET_9.Value & "','" & TagSrc.RI_DCH_RcpSET_10.Value & "','" & TagSrc.RI_DCH_RcpSET_11.Value & "','" & TagSrc.RI_DCH_RcpSET_12.Value & "','" & TagSrc.RI_DCH_RcpSET_13.Value & "','" & TagSrc.RI_DCH_RcpSET_14.Value & "','" & TagSrc.RI_DCH_RcpSET_15.Value & "','"
    strSql = strSql & TagSrc.RI_ECH_RcpSET_1.Value & "','" & TagSrc.RI_ECH_RcpSET_2.Value & "','" & TagSrc.RI_ECH_RcpSET_3.Value & "','" & TagSrc.RI_ECH_RcpSET_4.Value & "','" & TagSrc.RI_ECH_RcpSET_5.Value & "','" & TagSrc.RI_ECH_RcpSET_6.Value & "','" & TagSrc.RI_ECH_RcpSET_7.Value & "','" & TagSrc.RI_ECH_RcpSET_8.Value & "','" & TagSrc.RI_ECH_RcpSET_9.Value & "','" & TagSrc.RI_ECH_RcpSET_10.Value & "','" & TagSrc.RI_ECH_RcpSET_11.Value & "','" & TagSrc.RI_ECH_RcpSET_12.Value & "','" & TagSrc.RI_ECH_RcpSET_13.Value & "','" & TagSrc.RI_ECH_RcpSET_14.Value & "','" & TagSrc.RI_ECH_RcpSET_15.Value & "')"
    cmd.ActiveConnection = cnSrc
    rsSrc.Open strSql, cnSrc
   
    Exit Function
   
ErrHandler:
    MsgBox Err.Description
 
End Function
