' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
' * * * * * * * * * * * * * *         INSERT ON SAVE        * * * * * * * * * * * * *
' * * * * * * * * * * * * * * * * *       Q_Prod      * * * * * * * * * * * * * * * *
' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
Function QProdInsert(RcpID As String, cnSrc As ADODB.Connection)
    Dim strSql As String
    Dim cmd As New ADODB.Command
    Dim rsSrc As New ADODB.Recordset
    Dim AllTags As QProdGroup
    Dim TagSrc As QProdTags
    On Error GoTo ErrHandler
 
    AllTags = TGroup.LoadQProdGroup
    TagSrc = AllTags.DB
   
 
    strSql = " INSERT INTO Q_Prod VALUES('"
    strSql = strSql & RcpID & "','" & TagSrc.RcpGrade.Value & "','"
    strSql = strSql & TagSrc.Q_prdct_HEAT.Value & "','" & TagSrc.Q_prdct_MO.Value & "','" & TagSrc.Q_prdct_COIL.Value & "','" & TagSrc.Q_prdct_COILSn.Value & "','" & TagSrc.Q_prdct_SIZE.Value & "','" & TagSrc.Q_prdct_SPARE1.Value & "','" & TagSrc.Q_prdct_SPARE2.Value & "','" & TagSrc.Q_prdct_SPARE3.Value & "','" & TagSrc.Q_prdct_bil_DROP_count.Value & "','" & TagSrc.Q_prdct_bil_QUANT.Value & "','" & TagSrc.Q_prdct_bil_ROLLED.Value & "','" & TagSrc.Q_prdct_bil_SCHED.Value & "','" & TagSrc.Q_prdct_bil_SIZE.Value & "','" & TagSrc.Q_prdct_bil_WEIGHT.Value & "')"
    cmd.ActiveConnection = cnSrc
    rsSrc.Open strSql, cnSrc
   
    Exit Function
   
ErrHandler:
    MsgBox Err.Description
 
End Function
' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
' * * * * * * * * * * * * * *       UPDATE ON SAVE        * * * * * * * * * * * * * *
' * * * * * * * * * * * * * * * * *     Master        * * * * * * * * * * * * * * * *
' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
Function MasterUpdate(RcpID As String, cnSrc As ADODB.Connection)
    Dim strSql As String
    Dim cmd As New ADODB.Command
    Dim rsSrc As New ADODB.Recordset
    Dim TagSrc As MasterTags
    Dim DateValue As String
    On Error GoTo ErrHandler
   
    ' Records the current date and time when an update is made
    DateValue = Format(Date, "mmm d yyyy") & " " & Format(Time, "h:mm AM/PM")
 
    ' Loads all the tags for the Master table
    TagSrc = TGroup.LoadMasterTags
   
    'Updates all the coloumns in the table with the corresponding recipe ID
    strSql = "UPDATE [master] SET" & _
    "[RcpGrade] = '" & TagSrc.RcpGrade.Value & "',[RcpFinish]= '" & TagSrc.RcpFinish.Value & "',[RcpRough]= '" & TagSrc.RcpRough.Value & "',[RcpDsc] = '" & TagSrc.RcpDsc.Value & "',[RCPDate] = '" & DateValue & "' " & _
    "WHERE RcpID = '" & RcpID & "'"
    cmd.ActiveConnection = cnSrc
    rsSrc.Open strSql, cnSrc
   
    Exit Function
   
ErrHandler:
    MsgBox Err.Description
 
End Function
 
' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
