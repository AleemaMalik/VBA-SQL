    rsSrc.Open strSql, cnSrc
   
    Exit Function
   
ErrHandler:
    MsgBox Err.Description
   
End Function
' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
' * * * * * * * * * * * * * *         UPDATE ON SAVE        * * * * * * * * * * * * *
' * * * * * * * * * * * * * * * * *       Q_Prod        * * * * * * * * * * * * * * *
' * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
Function UpdateQProd(RcpID As String, cnSrc As ADODB.Connection)
Dim strSql As String
Dim cmd As New ADODB.Command
Dim rsSrc As New ADODB.Recordset
Dim AllTags As QProdGroup
Dim TagSrc As QProdTags
On Error GoTo ErrHandler
 
    AllTags = TGroup.LoadQProdGroup
    TagSrc = AllTags.DB
 
    strSql = "UPDATE [q_prod] SET" & _
    "[RcpGrade] = '" & TagSrc.RcpGrade.Value & "',[Q_prdct_HEAT] = '" & TagSrc.Q_prdct_HEAT.Value & "',[Q_prdct_MO] = '" & TagSrc.Q_prdct_MO.Value & "',[Q_prdct_COIL] = '" & TagSrc.Q_prdct_COIL.Value & "',[Q_prdct_COILSn] = '" & TagSrc.Q_prdct_COILSn.Value & "',[Q_prdct_SIZE] = '" & TagSrc.Q_prdct_SIZE.Value & "',[Q_prdct_SPARE1] = '" & TagSrc.Q_prdct_SPARE1.Value & "',[Q_prdct_SPARE2] = '" & TagSrc.Q_prdct_SPARE2.Value & "',[Q_prdct_SPARE3] = '" & TagSrc.Q_prdct_SPARE3.Value & "',[Q_prdct_bil_DROP_count] = '" & TagSrc.Q_prdct_bil_DROP_count.Value & "',[Q_prdct_bil_QUANT] = '" & TagSrc.Q_prdct_bil_QUANT.Value & "',[Q_prdct_bil_ROLLED] = '" & TagSrc.Q_prdct_bil_ROLLED.Value & "',[Q_prdct_bil_SCHED] = '" & TagSrc.Q_prdct_bil_SCHED.Value & "',[Q_prdct_bil_SIZE] = '" & TagSrc.Q_prdct_bil_SIZE.Value & "',[Q_prdct_bil_WEIGHT] = '" & TagSrc.Q_prdct_bil_WEIGHT.Value & "'" & _
    "WHERE RcpID = '" & RcpID & "'"
    cmd.ActiveConnection = cnSrc
    rsSrc.Open strSql, cnSrc
 
    Exit Function
   
ErrHandler:
    MsgBox Err.Description
 
End Function
